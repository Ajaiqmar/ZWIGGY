                       Height="50"
                       Width="50"/>
                <TextBlock Text="No Favourite Restaurants"
                           Style="{ThemeResource CaptionTextBlockStyle}"
                           FontSize="18"
                                       Margin="0,10,0,5"
                                       FontWeight="Light"
                           HorizontalAlignment="Center" />
