                      CornerRadius="5"
                      IsItemClickEnabled="False"
                      VerticalAlignment="Stretch"
                      SelectionMode="None"
                      Padding="0"
                      ScrollViewer.VerticalScrollBarVisibility="Hidden">
                        <ListView.ItemContainerStyle>
                            <Style TargetType="ListViewItem">
                                <Setter Property="HorizontalContentAlignment"
                                Value="Stretch" />
                                <Setter Property="Margin"
                                Value="0,0,0,10" />
                            </Style>
                        </ListView.ItemContainerStyle>
                        <ListView.ItemTemplate>
                            <DataTemplate x:DataType="mbo:OrderBObj">
                                <Grid HorizontalAlignment="Stretch"
                              Background="White"
                                      Loaded="OnListItemsLoaded"
                                      PointerPressed="OnOrderItemPointerPressed"
                                      BorderThickness="0.5"
                              Height="75"
                              CornerRadius="5"
                                      PointerEntered="OnOrderItemPointerEnter"
                                      PointerExited="OnOrderItemPointerExited">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>

                                    <Grid CornerRadius="5"
                                          Margin="10">
