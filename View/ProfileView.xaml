                                              OnGridItemClick="OnRestaurantSelect"/>
            </Grid>

            <!-- ADDRESS CONTENT -->

            <Grid Name="AddressContentGrid"
                  Visibility="{x:Bind _profileVM.IsAddressNotEmpty,Mode=OneWay}">
                <controls:AdaptiveGridView Name="AddressGridView"
                                                                   Margin="5,0,5,0"
                                                                   SelectionMode="None"
                                                                   ItemHeight="150"
                                                                   DesiredWidth="350"
                                                                   ScrollViewer.VerticalScrollBarVisibility="Hidden"
                                                                   ItemsSource="{x:Bind _profileVM.Addresses,Mode=OneWay}">
                    <controls:AdaptiveGridView.ItemContainerStyle>
                        <Style TargetType="GridViewItem">
                            <Setter Property="Margin" Value="5"/>
                        </Style>
                    </controls:AdaptiveGridView.ItemContainerStyle>
                    <controls:AdaptiveGridView.ItemTemplate>
                        <DataTemplate x:DataType="m:Address">
                            <Grid CornerRadius="5"
                                                          HorizontalAlignment="Stretch"
                                                          Background="White">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>

                                <StackPanel Margin="20,10,20,10">
                                    <TextBlock Text="{Binding Nickname}"
                                                                       Style="{StaticResource HeaderTextBlockStyle}"
                                                                       TextTrimming="CharacterEllipsis"
                                                                       TextWrapping="NoWrap"
                                                                       FontSize="18" />
                                    <TextBlock Text="{Binding AddressDescription}"
                                                                       Margin="0,10,0,0"
                                                                       FontWeight="Light"
                                                                       TextTrimming="CharacterEllipsis"
                                                                       TextWrapping="NoWrap"
                                                                       Foreground="#5e5e5e" />
                                </StackPanel>

                                <Button Grid.Row="2"
                                                                Content="Remove"
                                                                CornerRadius="5"
                                                                Margin="20,0,0,20"
                                                                Foreground="White"
                                                                Background="{StaticResource ApplicationBaseAccentColor}"
                                                                BorderThickness="0"
                                                                Click="OnRemoveAddressSelected"
                                                                Padding="10,6,10,6">
                                    <Button.Resources>
                                        <ResourceDictionary>
                                            <ResourceDictionary.ThemeDictionaries>
                                                <ResourceDictionary x:Key="Default">
                                                    <SolidColorBrush x:Key="ButtonBackgroundPointerOver"
                                                                                             Color="{Binding Source={StaticResource ApplicationBaseHoverAccentColor},Path=Color}"/>
                                                    <SolidColorBrush x:Key="ButtonBackgroundPressed"
                                                                                             Color="{Binding Source={StaticResource ApplicationBaseHoverAccentColor},Path=Color}"/>
                                                    <SolidColorBrush x:Key="ButtonForegroundPressed"
                                                                                             Color="White" />
                                                    <SolidColorBrush x:Key="ButtonForegroundPointerOver"
                                                                                             Color="White" />
                                                </ResourceDictionary>
                                            </ResourceDictionary.ThemeDictionaries>
                                        </ResourceDictionary>
                                    </Button.Resources>
                                </Button>
                            </Grid>
                        </DataTemplate>
                    </controls:AdaptiveGridView.ItemTemplate>
                </controls:AdaptiveGridView>
            </Grid>

            <!-- SETTINGS CONTENT -->

            <Grid Name="SettingsContentGrid"
                  Visibility="{x:Bind _profileVM.IsSettingsVisible,Mode=OneWay}">
