                                                   FontSize="12"
                                                       Margin="0,0,5,0"
                                                   FontWeight="Light"
                                                   Opacity="0.5"/>
                                            <TextBlock Text="{x:Bind OrderPlacedDateAndTime.ToLongTimeString(),Mode=OneWay}"
                                                   FontSize="12"
                                                       Margin="0,0,5,0"
                                                   FontWeight="Light"
                                                   Opacity="0.5"/>
                                            <TextBlock Text="{x:Bind OrderPlacedDateAndTime.ToLongDateString(),Mode=OneWay}"
                                                   FontSize="12"
                                                   FontWeight="Light"
                                                   Opacity="0.5"/>
                                        </StackPanel>
                                    </StackPanel>
                                </Grid>
                            </DataTemplate>
                        </ListView.ItemTemplate>
                    </ListView>
                </Grid>
                <uc:OrderDetailsControl x:Name="OrderDetailsPane"
                    Grid.Column="1"
                                        BackButtonPressed="OnBackButtonPressed"
                                        Margin="-10,-10,0,0"
                                        DetailsExpandButtonVisibility="true"
                                        BackButtonVisibility="false"
                                        OrderDetails="{x:Bind _profileVM.OrderDetails,Mode=OneWay}"/>
