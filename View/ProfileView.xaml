                           Style="{ThemeResource CaptionTextBlockStyle}"
                           FontSize="14"
                                       Width="300"
                                       HorizontalTextAlignment="Center"
                                       TextWrapping="Wrap"
                                       Opacity="0.5"
                                       FontWeight="Light"
                           HorizontalAlignment="Center" />
            </StackPanel>

