                                    <Rectangle x:Name="WindowedPopupPadding"
                                                   Grid.Row="1"
                                                   Height="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CommandBarTemplateSettings.ContentHeight}" />
                                </Grid>
                            </Popup>

